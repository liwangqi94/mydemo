<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>FirstController</servlet-name>
  	<display-name>FirstController</display-name>
  	<description></description>
  	<servlet-class>com.njbdqn.mydemo.controller.FirstController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>TestController</servlet-name>
  	<display-name>TestController</display-name>
  	<description></description>
  	<servlet-class>com.njbdqn.mydemo.controller.TestController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FirstController</servlet-name>
  	<url-pattern>/first</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>TestController</servlet-name>
  	<url-pattern>/test</url-pattern>
  </servlet-mapping>
</web-app>
